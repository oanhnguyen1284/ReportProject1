  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CR">
            <Value>=Parameters!CR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        YEAR(SalesLT.SalesOrderHeader.DueDate) AS YEAR, MONTH(SalesLT.SalesOrderHeader.DueDate) AS MONTH, SalesLT.SalesOrderHeader.TotalDue, 
                         SalesLT.Address.CountryRegion, SalesLT.Address.StateProvince
FROM            SalesLT.SalesOrderDetail INNER JOIN
                         SalesLT.SalesOrderHeader ON SalesLT.SalesOrderDetail.SalesOrderID = SalesLT.SalesOrderHeader.SalesOrderID INNER JOIN
                         SalesLT.Address ON SalesLT.SalesOrderHeader.ShipToAddressID = SalesLT.Address.AddressID AND 
                         SalesLT.SalesOrderHeader.BillToAddressID = SalesLT.Address.AddressID
WHERE        (SalesLT.Address.CountryRegion = @CR)</CommandText>
      </Query>
      <Fields>
        <Field Name="YEAR">
          <DataField>YEAR</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MONTH">
