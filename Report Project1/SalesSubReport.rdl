  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CR">
            <Value>=Parameters!CR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        SalesLT.SalesOrderHeader.rowguid, SalesLT.SalesOrderHeader.ModifiedDate, SalesLT.SalesOrderHeader.SubTotal, SalesLT.Address.CountryRegion, 
                         SalesLT.Address.StateProvince
FROM            SalesLT.SalesOrderHeader INNER JOIN
                         SalesLT.Address ON SalesLT.SalesOrderHeader.ShipToAddressID = SalesLT.Address.AddressID AND 
                         SalesLT.SalesOrderHeader.BillToAddressID = SalesLT.Address.AddressID
WHERE        (SalesLT.Address.CountryRegion = @CR)</CommandText>
      </Query>
