  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        SalesLT.SalesOrderHeader.TotalDue, YEAR(SalesLT.SalesOrderHeader.DueDate) AS YEAR, MONTH(SalesLT.SalesOrderHeader.DueDate) AS Month, 
                         SalesLT.Address.CountryRegion, SalesLT.Address.StateProvince, SalesLT.Address.PostalCode
FROM            SalesLT.SalesOrderHeader INNER JOIN
                         SalesLT.Address ON SalesLT.SalesOrderHeader.ShipToAddressID = SalesLT.Address.AddressID AND 
                         SalesLT.SalesOrderHeader.BillToAddressID = SalesLT.Address.AddressID</CommandText>
      </Query>
      <Fields>
        <Field Name="TotalDue">
          <DataField>TotalDue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="YEAR">
          <DataField>YEAR</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CountryRegion">
          <DataField>CountryRegion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StateProvince">
